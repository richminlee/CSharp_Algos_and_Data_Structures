1313. Decompress Run-Length Encoded List

public class Solution {
    public int[] DecompressRLElist(int[] nums) {
        List<int> result = new List<int>();
        for(int i = 0; i < nums.Length; i+=2){
            for(int j = 0; j < nums[i]; j++){
                result.Add(nums[i+1]);
            }
        }
        return result.ToArray();
    }
}
